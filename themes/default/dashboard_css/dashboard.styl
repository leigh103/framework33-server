
@font-face {
    font-family: 'Nunito';
    src: url('fonts/NunitoSans-Light.ttf');
    src: url('fonts/NunitoSans-Light.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Nunito';
