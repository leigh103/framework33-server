<% var value; %>
<% if (item.value == 'user_id'){ %>
    <% value = user._id %>
<% } %>

<%
    if (typeof obj_prefix == 'undefined' || obj_prefix == false){
        obj_prefix = ''
    } else {
        obj_prefix = obj_prefix+'.'
    }
%>

<input type="hidden" app-model="new.<%- obj_prefix %><%- item.name %>" value="<%- value %>">
