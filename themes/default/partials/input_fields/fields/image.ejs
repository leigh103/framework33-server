<%
    if (typeof obj_prefix == 'undefined' || obj_prefix == false){
        obj_prefix = ''
        obj_prefix_snake = ''
    } else {
        obj_prefix_snake = obj_prefix+'_'
        obj_prefix = obj_prefix+'.'
    }
%>
<div class="input-image field-<%- obj_prefix %><%- item.name %>">
    <label><%- view.functions.parseName(item.name) %></label>
    <label for="block_bg_image" app-click="showMediaLibrary('new.<%- obj_prefix %><%- item.name %>')"><img src="/images/Product_Placeholder.svg" app-src="new.<%- obj_prefix %><%- item.name %>" class="<%- item.name %>"></label>
</div>
