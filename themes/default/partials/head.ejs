<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content = "width = device-width, initial-scale = 1.0, minimum-scale = 1, maximum-scale = 1, user-scalable = no" />

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">

    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="white-translucent">
    <% if (typeof dashboard != 'undefined'){ %>
        <link rel="manifest" href="/dashboard-manifest.json">
    <% } else { %>
        <link rel="manifest" href="/manifest.json">
    <% } %>

    <meta name="msapplication-TileImage" content="/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <meta name="twitter:card" content="summary" />

    <% if (typeof meta == 'object'){ %>

        <% if (meta.title){ %>
            <meta name="apple-mobile-web-app-title" content="<%= meta.title %>" />
            <meta property="og:title" content="<%= meta.title %>">
            <title><%= meta.title %></title>
        <% } %>
        <% if (meta.image){ %><meta property="og:image" content="<%= meta.image %>"><% } %>
        <% if (meta.url){ %><meta property="og:url" content="<%= meta.url %>"><% } %>
        <% if (meta.updated_time){ %><meta property="og:updated_time" content="<%= meta.updated_time %>"><% } %>
        <% if (meta.description){ %>
            <meta property="og:description" content="<%= meta.description %>">
            <meta name="description" content="<%= meta.description %>">
        <% } %>

    <% } else if (typeof view.meta == 'object'){ %>

        <title><%= view.meta.title %></title>
        <meta name="apple-mobile-web-app-title" content="<%= view.meta.title %>" />
        <meta name="description" content="<%= view.meta.description %>">

        <meta property="og:title" content="<%= view.meta.title %>">
        <meta property="og:description" content="<%= view.meta.description %>">
        <meta property="og:image" content="<%= view.meta.logo %>">

    <% } %>

    <script src="https://kit.fontawesome.com/e7faa1906b.js" crossorigin="anonymous"></script>

    <link href="/style/framework-33/v2.0/framework33.css?v=3.0.65" rel="stylesheet">
    <link href="/style/fonts.css?v=1614765140082" rel="stylesheet">

    <% if (typeof dashboard != 'undefined'){ %>
        <link href="/style/dashboard.css?v=1621899293796" rel="stylesheet">
    <% } else { %>
        <link href="/style/style.css?v=1621962837498" rel="stylesheet">
    <% } %>

    <% if (typeof stripe_id != 'undefined'){ %>
        <script src="https://js.stripe.com/v3/"></script>
    <% } %>

    <% if (typeof include_styles != 'undefined') { %>

        <% include_styles.forEach((style,i)=>{ %>
            <% if (fs.existsSync(view.basedir+'/components/'+style)) { %>
                <%- include(view.basedir+'/components/'+style) %>
            <% } %>
        <% }) %>

    <% } %>

</head>
